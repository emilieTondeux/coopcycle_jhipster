<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.9.xsd">
    <!--
        Added the constraints for entity Restaurant.
    -->
    <changeSet id="20210411073620-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="acount_id"
                                 baseTableName="restaurant"
                                 constraintName="fk_restaurant_acount_id"
                                 referencedColumnNames="id"
                                 referencedTableName="acount"/>

        <addForeignKeyConstraint baseColumnNames="cooperative_id"
                                 baseTableName="restaurant"
                                 constraintName="fk_restaurant_cooperative_id"
                                 referencedColumnNames="id"
                                 referencedTableName="cooperative"/>

    </changeSet>
</databaseChangeLog>
